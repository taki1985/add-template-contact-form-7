  "main": "index.js",
  "scripts": {
    "copy/php": "cpx \"./src/*.php\" ../../../plugins/add-template-for-contact-form-7/",
    "copy/lib": "cpx \"./src/lib/*\" ../../../plugins/add-template-for-contact-form-7/lib/",
    "copy/languages": "cpx \"./src/languages/*\" ../../../plugins/add-template-for-contact-form-7/languages/",
    "copy/image": "cpx \"./src/images/*.{jpg,png,gif}\" ../../../plugins/add-template-for-contact-form-7/assets/images/",
    "copy/uc": "cpx \"./src/plugin-update-checker/\" ../../../plugins/add-template-for-contact-form-7/plugin-update-checker/",
    "copy/readme": "cpx \"./src/readme.txt\" ../../../plugins/add-template-for-contact-form-7/",
    "copy": "npm-run-all -p copy/*",
    "watch/copy": "watch 'npm run copy' ./src/",
    "js/babel": "babel ./src/js/index.js --out-file ../../../plugins/add-template-for-contact-form-7/assets/index.js --compact=true",
    "js": "npm-run-all -p js/*",
    "watch/js": "watch 'npm run js' ./src/",
    "sass/style": "sass src/sass/style.scss:../../../plugins/add-template-for-contact-form-7/assets/style.css --no-source-map",
    "sass": "npm-run-all -p sass/*",
    "watch/sass": "watch 'npm run sass' ./src/sass",
    "watch/server": "browser-sync start --proxy 'localhost:8888' --port 3000 -w",
    "watch": "npm-run-all -p watch/*",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
